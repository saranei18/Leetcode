class Solution {
public:
    vector<int>v;
    vector<vector<int>>res;
    void number_permute(int start,int end,vector<int>&nums)
    {
        if(start==end)
        {
            for(int i=0;i<=end;i++)
                v.push_back(nums[i]);
            res.push_back(v);
            v.clear();
        }
        else
        {
            for(int i=start;i<=end;i++)
            {
                int t=nums[i];
                nums[i]=nums[start];
                nums[start]=t;
                number_permute(start+1,end,nums);
                t=nums[i];
                nums[i]=nums[start];
                nums[start]=t;
                
            }
        }
    }
    vector<vector<int>> permute(vector<int>& nums) 
    {
        number_permute(0,nums.size()-1,nums);
        return res;
    }
};
