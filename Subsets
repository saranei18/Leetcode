class Solution {
public:
    vector<vector<int>>result;
    void permute(vector<int>&nums,vector<int>&temp)
    {
        if(nums.size()==0)
            result.push_back(temp);
        else
        {
            //choose
            
            int t=nums[0];
            vector<int>::iterator it;
            it = nums.begin();
            nums.erase(it);
            temp.push_back(t);
            
            //explore with
            permute(nums,temp);
            temp.pop_back();
            //explore without
            
            permute(nums,temp);
            auto itr = nums.insert(nums.begin(), t);
        }
    }
    vector<vector<int>> subsets(vector<int>& nums) 
    {
        vector<int>temp;
        permute(nums,temp);
        return result;
    }
};
